@model IEnumerable<BestApp.Entities.Track>

@{
    ViewBag.Title = "MyTracks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MyTracks</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StartHour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Start)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CarSeats)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailAddress)
        </th>
        <th></th>
    </tr>

    @foreach (var track in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => track.StartHour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => track.Start)
            </td>
            <td>
                @Html.DisplayFor(modelItem => track.Stop)
            </td>
            <td>
                @Html.DisplayFor(modelItem => track.CarSeats)
            </td>
            <td>
                @Html.DisplayFor(modelItem => track.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => track.EmailAddress)
            </td>
        </tr>
    }

</table>